{
  "name": "grades-1-and-3-tx-tm-records-backend",
  "version": "1.0.0",
  "description": "Grades 1 and 3 TX and TM records - Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "seed": "ts-node src/database/seed.ts",
    "extract-excel": "ts-node scripts/extract-excel-data.js",
    "upload-evaluators": "ts-node -r tsconfig-paths/register src/scripts/uploadEvaluators.ts",
    "migrate:ntp-date": "ts-node -r tsconfig-paths/register src/scripts/add_ntp_date_field.ts",
    "verify:ntp-date": "ts-node -r tsconfig-paths/register src/scripts/verify_ntp_date_field.ts"
  },
  "keywords": [
    "book",
    "management",
    "api",
    "nodejs",
    "express",
    "postgresql"
  ],
  "author": "Grades 1 and 3 TX and TM records",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.20.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "tsc-alias": "^1.8.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}